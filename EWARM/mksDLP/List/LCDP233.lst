     34          unsigned LCD_Color2Index_233(LCD_COLOR Color) {
     35            int r, g, b;
     36            r = Color & 255;
     37            g = (Color >> 8 ) & 255;
     38            b = Color >> 16;
     39            r = (r * 7 + 127) / 255;
     40            g = (g * 7 + 127) / 255;
     41            b = (b + 42) / 85;
     42            return r + (g << 3) + (b << 6);
