     49          LCD_COLOR LCD_Index2Color_233(int Index) {
     50            int r, g, b;
     51            r = (Index & 7) * 255 / 7;
     52            g = ((Index >> 3) & 7) * 255 / 7;
     53            b = ((Index >> 6) & 3) * 85;
     54            return r + (g << 8) + (((U32)b) << 16);
