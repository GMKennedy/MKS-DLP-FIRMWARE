     37          void GUI_AA_DrawPolyOutline(const GUI_POINT* pSrc, int NumPoints, int Thickness, int x, int y) {
     38            U8 PrevDraw;
     39            GUI_POINT aiTemp[10];
     40            GUI_EnlargePolygon(&aiTemp[0], pSrc, NumPoints, Thickness);
     41            GUI_AA_FillPolygon( &aiTemp[0], NumPoints, x, y );
     42            PrevDraw = LCD_SetDrawMode(GUI_DRAWMODE_REV);
     43          /* Copy points due to const qualifier */
     44            memcpy(aiTemp, pSrc, NumPoints*2*sizeof(GUI_POINT));
     45            GUI_AA_FillPolygon( aiTemp, NumPoints, x, y );
     46            LCD_SetDrawMode(PrevDraw);
     47          }
