     33          static void _ModifyLUT(int RFaktor, int GFaktor, int BFaktor) {
     34            int NumColors = LCD_GetDevCap(LCD_DEVCAP_NUMCOLORS);
     35            int i;
     36            for (i = 0; (i < NumColors) && !GUIDEMO_CheckCancel(); i++) {    
     37              U32 Color = LCD_GetDevCap(LCD_DEVCAP_COLOR + i);
     38              U32 R = Color & 0xff;
     39              U32 G = (Color >> 8)  & 0xff;
     40              U32 B = (Color >> 16) & 0xff;
     41              /* Now modify color */
     42              R = (R * RFaktor) / 100; if (R > 255) R = 255;
     43              G = (G * GFaktor) / 100; if (G > 255) G = 255;
     44              B = (B * BFaktor) / 100; if (B > 255) B = 255;
     45              /* Write modified color into lookup table */
     46              Color = R | (G << 8) | (B << 16);
     47              GUI_SetLUTEntry((U8)i, Color);    
     48            }
     49          }
