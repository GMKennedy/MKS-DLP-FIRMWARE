     57          
     58          void GUIDEMO_DemoLUT(void) {
     59            int i;
     60            GUIDEMO_ShowIntro("Modify LUT", "... after drawing color bars");
     61            GUIDEMO_ShowColorBar();
     62          	for (i = 100; (i >= 0) && !GUIDEMO_CheckCancel(); i -= 2) {
     63              _ModifyLUT(100, 100, i);
     64              GUI_Delay(20);
     65          	}
     66          	for (i = 100; (i >= 0) && !GUIDEMO_CheckCancel(); i -= 2) {
     67              _ModifyLUT(100, i, 0);
     68              GUI_Delay(20);
     69          	}
     70          	for (i = 100; (i >= 0) && !GUIDEMO_CheckCancel(); i -= 2) {
     71              _ModifyLUT(i, i, i);
     72              GUI_Delay(20);
     73          	}
     74            LCD_InitLUT();
     75            GUIDEMO_Wait();
     76          }
